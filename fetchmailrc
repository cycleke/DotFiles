set daemon 600
set logfile /home/cycleke/.fetchmaillog

poll imap.gmail.com
  protocol IMAP service 993
  plugin "socat STDIO SOCKS4A:127.0.0.1:imap.gmail.com:993,socksport=1080"
  user "XXX@gmail.com" password ""
  options
  keep ssl

poll outlook.office365.com
 protocol POP3 service 995
 user "XXX@hotmail.com" password ""
 options
 keep ssl

mimedecode
mda "/usr/bin/procmail -d %T"
